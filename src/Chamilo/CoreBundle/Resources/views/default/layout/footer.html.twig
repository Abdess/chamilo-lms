{#<footer {% block footer_open_attributes %}{% endblock footer_open_attributes %}>#}
<footer class="sticky-footer">
    <div class="pre-footer"></div>
    <div class="sub-footer">
        <div class="container">
            <div class="row">
                <div id="footer_left" class="col-md-9">     
                    <div class="partners">
                        <a href="http://www.bosch-stiftung.de" target="_blank">
                            <img src="{{ asset('css/themes/'~ theme ~'/images/rbs_logo_rgb.png') }}"/>
                        </a>
                        <a href="http://www.cavilam.com" target="_blank">
                            <img src="{{ asset('css/themes/'~ theme ~'/images/logo_cavilam.png') }}"/>
                        </a>
                        <a href="http://www.dw.com" target="_blank">
                            <img src="{{ asset('css/themes/'~ theme ~'/images/logo-dw.png') }}"/>
                        </a>
                        <a href="https://www.cdc.de/" target="_blank">
                            <img src="{{ asset('css/themes/'~ theme ~'/images/carlduisberg.png') }}"/>
                        </a>
                    </div>
                </div>
                <div id="footer_right" class="col-md-3">

                    <div id="admin_name">
                        <a href="{{ "URLOFAJ" | trans }}">{{ "OFAJ" | trans }}</a>
                    </div>

                    <div id="software_name">
                        <a href="" target="_blank">
                            {{ "PoweredByX" | trans | format(software_name) }}
                        </a>
                        &copy; {{ "now"|date("Y") }}
                    </div>
                    <div id="mentions_legales">
                            <a href="{{ "MentionsLegalesLink" | trans }}" target="_blank">{{ "MentionsLegales" | trans }}</a> - <a href="{{ "CGULink" | trans }}" target="_blank">{{ "CGU" | trans }}</a>
                    </div>

                    {#   Plugins for footer section  #}
                    {% if plugin_footer_right is not null %}
                        <div id="plugin_footer_right">
                            {{ plugin_footer_right }}
                        </div>
                    {% endif %}
                    &nbsp;
                </div>
            </div>
        </div>
    </div>
</footer>

{{ footer_extra_content }}
