{
    "name": "chamilo/chamilo-lms",
    "description": "E-learning and collaboration software",
    "type": "project",
    "homepage": "http://www.chamilo.org",
    "license": "GPL-3.0",
    "support": {
        "forum": "https://chamilo.org/forum",
        "irc": "irc://irc.freenode.org/chamilo",
        "source": "https://github.com/chamilo/chamilo-lms",
        "docs": "https://github.com/chamilo/docs",
        "issues": "https://github.com/chamilo/chamilo-lms/issues"
    },
    "autoload": {
        "psr-4": {
            "Chamilo\\": "src/",
            "Application\\": "app/"
        },
        "classmap": [
            "main/auth",
            "main/admin",
            "main/cron/lang",
            "main/course_description",
            "main/dropbox",
            "main/exercise",
            "main/gradebook/lib",
            "main/lp",
            "main/inc/lib",
            "plugin",
            "main/install",
            "main/survey",
            "main/inc/lib/hook"
        ]
    },
    "config": {
      "component-dir": "public/assets",
      "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ywarnier/iCalcreator.git"
        },
        {
           "type": "vcs",
            "url": "git@github.com:jmontoyaa/MigrationBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:jmontoyaa/PHPWord.git"
        },
        {
           "type": "vcs",
                "url": "git@github.com:jmontoyaa/SyliusFlowBundle.git"
        },
        {
           "type": "vcs",
            "url": "git@github.com:chamilo/SyliusSettingsBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/chamilo/pclzip"
        }
    ],
